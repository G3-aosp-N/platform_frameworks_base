<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Volume stream title names -->
    <string name="stream_title_call">Call</string>
    <string name="stream_title_system">System</string>
    <string name="stream_title_ring">Ring</string>
    <string name="stream_title_media">Media</string>
    <string name="stream_title_alarm">Alarm</string>
    <string name="stream_title_notification">Notification</string>
    <string name="stream_title_bluetooth">Bluetooth</string>

    <!-- Wi-Fi hotspot label when enabled -->
    <plurals name="wifi_hotspot_connected_clients_label">
        <item quantity="one">%1$d client</item>
        <item quantity="other">%1$d clients</item>
    </plurals>

    <!-- Indication on the keyguard that is shown when the device is charging with a dash charger. Should match keyguard_plugged_in_dash_charging [CHAR LIMIT=40]-->
    <string name="keyguard_indication_dash_charging_time">Dash Charging (<xliff:g id="charging_time_left" example="4 hours and 2 minutes">%s</xliff:g> until full)</string>
    <string name="keyguard_plugged_in_dash_charging">Dash Charging</string>

</resources>
